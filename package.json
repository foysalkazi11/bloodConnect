{
  "name": "blood-donation-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start --clear",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild --clean",
    "dev:client": "expo start --dev-client -c",
    "dev:client:local": "expo start --dev-client --local",
    "android:build": "eas build --platform android --profile preview",
    "ios:build": "eas build --platform ios --profile preview",
    "android:submit": "eas submit --platform android --profile preview",
    "ios:submit": "eas submit --platform ios --profile preview",
    "android:publish": "eas submit --platform android --profile production",
    "ios:publish": "eas submit --platform ios --profile production",
    "android:build:local": "eas build -p android --profile development --local",
    "ios:build:local": "eas build -p ios --profile development --local"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
