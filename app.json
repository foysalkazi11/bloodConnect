{
  "expo": {
    "name": "BloodLink",
    "slug": "blood-link",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "bloodlink",
    "description": "Join the BloodLink community! Connect with blood donors, manage donation clubs, organize events, and inspire others to save lives through social activities. Build a network of lifesavers in your community.",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2626"
    },
    "android": {
      "package": "com.foysalkazi11.bloodlink",
      "versionCode": 1,
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#DC2626"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.expo.io"
            },
            {
              "scheme": "bloodlink"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.foysalkazi11.bloodlink",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "bloodlink",
            "CFBundleURLSchemes": ["bloodlink"]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "BloodLink needs camera access to let you take photos for your profile and share donation stories with your community.",
        "NSPhotoLibraryUsageDescription": "BloodLink needs photo library access to let you select images for your profile and share inspiring donation stories with your community.",
        "NSLocationWhenInUseUsageDescription": "BloodLink uses your location to help you find nearby blood donors, donation clubs, and community events.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "BloodLink uses your location to help you find nearby blood donors, donation clubs, and community events.",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-web-browser",
        {
          "mayInitWithUrl": false
        }
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#DC2626"
        }
      ],
      [
        "expo-build-properties",
